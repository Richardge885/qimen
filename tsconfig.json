{
    "include": [
        "./src/*.ts"
    ],
    "compilerOptions": {
        "target": "ES2016",
        "module": "commonjs",
        "rootDir": "./src",
        "rootDirs": [
            "./src"
        ],
        "outDir": "./dist",
        "removeComments": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "references": [
        {
            "path": "./src/render/tsconfig_for_render.json"
        },
    ]
}
