{
    "include": [
        "./*.ts"
    ],
    "compilerOptions": {
        "target": "ES2016",
        "module": "commonjs",
        "composite": true,
        "outDir": "../../dist/public/js",
        "removeComments": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    }
}
